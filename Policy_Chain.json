{
 "policies_config": [
  {
   "name": "headers",
   "version": "builtin",
   "configuration": {
    "response": [
     {
      "value_type": "plain",
      "op": "set",
      "header": "Cache-Control",
      "value": "no-cache,no-store"
     },
     {
      "value_type": "plain",
      "op": "set",
      "header": "Pragma",
      "value": "no-cache"
     },
     {
      "value_type": "plain",
      "op": "set",
      "header": "Expires",
      "value": "0"
     }
    ]
   },
   "enabled": true
  },
  {
   "name": "cors",
   "version": "builtin",
   "configuration": {
    "allow_headers": [
     "*"
    ],
    "allow_methods": [
     "OPTIONS",
     "GET",
     "POST",
     "PUT",
     "PATCH"
    ],
    "allow_origin": "*"
   },
   "enabled": true
  },
  {
   "name": "apicast",
   "version": "builtin",
   "configuration": {},
   "enabled": true
  },
  {
   "name": "url_rewriting",
   "version": "builtin",
   "configuration": {
    "commands": [
     {
      "op": "sub",
      "regex": "/context/v1/offers",
      "replace": "/handshake/v1/context"
     }
    ]
   },
   "enabled": true
  }
 ]
}